global.IS_SERVER_REQUEST = false;

var React = require('react');
var ReactDOM = require('react-dom');
var env = require('./services/env');

require('./services/analytics')();

env.set( { 
  debugMode: <%= debug %>,
  browser: true,
  server: false,
  appName: '<%= app %>',
  rpcHost: 'http://<%= apihost %>/api/',
  queryHost: 'http://<%= apihost %>/api/query?',
  queriesHost: 'http://<%= apihost %>/api/queries?',
  selfHost: '<%= satellite_host %>' ? 'http://<%= satellite_host %>/' : '/'
});

var app = require('./<%= app%>');

ReactDOM.render( React.createElement(app),  document.getElementById('content') );

